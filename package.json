{
  "name": "aod-studio-firebase-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.13.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.13"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "install:all": "yarn && cd functionsES6 && yarn",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build": "react-scripts build && yarn run build:functions",
    "build:functions": "babel 'functionsES6' --out-dir 'functions' --presets=es2015 --copy-files --ignore 'node_modules' && cd functions && yarn",
    "deploy": "yarn run build && firebase deploy"
  }
}
